    "\n",
    "def reverse_integer(n):\n",
    "\n",
    "    reversed_integer = 0\n",
    "\n",
    "    while n > 0:\n",
    "        remainder = n % 10\n",
    "        reversed_integer = reversed_integer*10 + remainder\n",
    "        n = n // 10\n",
    "\n",
    "    return reversed_integer\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(reverse_integer(12345678))"
   ]
